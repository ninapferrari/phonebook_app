<div class="jumbotron">
  <h1 class="display-4">
    <%= @contact.first_name %>
    <%= @contact.last_name %>
  </h1>
  <p class="lead">
    <%= @contact.phone %>    
    <%= @contact.notes %>

  </p>
  <br/>

  <%= link_to 'Edit', edit_contact_path(@contact), class:"btn btn-secondary" %>

<%= button_to 'Delete', @contact, method: :delete, class: "btn btn-danger" %>
   
<%= link_to 'Back', contacts_path, class:"btn btn-dark" %>

</div>